[user]
    name = Rich Zurad
    email = rzurad@gmail.com
[core]
    editor = vim
[alias]
    lol = log --pretty=oneline --abbrev-commit --graph --decorate
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
